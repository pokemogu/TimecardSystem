{
  "name": "timecard-backend",
  "version": "0.7.0",
  "engines": {
    "npm": ">=8.3.1",
    "node": ">=16.14.0"
  },
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist/*",
    "typecheck": "cross-env NODE_ENV=development tsc --noEmit",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production tsc",
    "postbuild": "ncp src/worker_wrapper.js dist/worker_wrapper.js",
    "preesbuild": "npm run clean",
    "esbuild": "cross-env NODE_ENV=production node esbuild.js",
    "postesbuild": "ncp src/worker_wrapper.js dist/worker_wrapper.js",
    "prebuild-docker": "npm run esbuild",
    "build-docker": "docker compose build --force-rm --no-cache",
    "prestart-docker": "npm run build-docker",
    "start-docker": "docker compose up -d",
    "stop-docker": "docker compose down --rmi all",
    "start": "cross-env NODE_ENV=production node --enable-source-maps dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node src/index.ts",
    "production": "cross-env NODE_ENV=production ts-node --transpile-only src/index.ts",
    "dev-auto": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "up-mysql": "cross-env NODE_ENV=development mysql/up.sh",
    "down-mysql": "cross-env NODE_ENV=development mysql/down.sh",
    "migrate:latest-dev": "cross-env NODE_ENV=development knex migrate:latest --env development",
    "migrate:latest-production": "cross-env NODE_ENV=production knex migrate:latest --env production",
    "migrate:rollback-dev": "cross-env NODE_ENV=development knex migrate:rollback --env development",
    "migrate:rollback-production": "cross-env NODE_ENV=production knex migrate:rollback --env production",
    "seed-dev": "cross-env NODE_ENV=development knex seed:run --env development --specific development-init.js",
    "seed-production": "cross-env NODE_ENV=production knex seed:run --env production --specific production-init.js",
    "migrate:make": "cross-env NODE_ENV=development knex migrate:make",
    "test": "jest --passWithNoTests --forceExit",
    "link-node-windows": "cross-env NODE_ENV=production npm link --production node-windows",
    "install-windows-service": "cross-env NODE_ENV=production node windows-service.js install",
    "uninstall-windows-service": "cross-env NODE_ENV=production node windows-service.js uninstall",
    "start-windows-service": "cross-env NODE_ENV=production node windows-service.js start",
    "stop-windows-service": "cross-env NODE_ENV=production node windows-service.js stop",
    "configure-service": "cross-env NODE_ENV=production node configure.js"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "globalSetup": "./jest.setup.js",
    "testTimeout": 60000
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pokemogu/TimecardSystem.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pokemogu/TimecardSystem/issues"
  },
  "homepage": "https://github.com/pokemogu/TimecardSystem#readme",
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "cross-env": "^7.0.3",
    "detect-ts-node": "^1.0.5",
    "dotenv": "^16.0.0",
    "enquirer": "^2.3.6",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-bearer-token": "^2.4.0",
    "http-errors": "^1.8.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.4",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.3",
    "run-script-os": "^1.1.6",
    "uuid-apikey": "^1.5.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.180",
    "@types/node": "^17.0.26",
    "@types/nodemailer": "^6.4.4",
    "esbuild": "^0.14.27",
    "jest": "^27.5.1",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  },
  "bundledDependencies": [
    "cross-env",
    "detect-ts-node",
    "dotenv",
    "express",
    "express-async-handler",
    "express-bearer-token",
    "http-errors",
    "jsonwebtoken",
    "knex",
    "lodash",
    "mysql2",
    "nodemailer"
  ]
}