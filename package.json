{
  "name": "timecard-system",
  "version": "v0.1.1",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev-backend": "npm run --workspace=backend dev-auto",
    "dev-frontend": "npm run --workspace=frontend dev",
    "dev": "npm-run-all --parallel dev-backend dev-frontend"
  },
  "private": true,
  "msw": {
    "workerDirectory": "frontend/public"
  },
  "dependencies": {
    "backend": "^0.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}